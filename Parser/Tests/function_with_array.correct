function processAudio <- (files: audio[]): audio {
    return MIX(files[0], files[1], files[2]);
}

function main <-() {
    tracks: audio[3] <- 0;
    tracks[0] = load "track1.wav";
    tracks[1] = load "track2.wav";
    tracks[2] = load "track3.wav";
    finalTrack = processAudio(tracks);
    play finalTrack;
}
