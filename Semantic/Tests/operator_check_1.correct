main {
    result: float <- 10 + 5 * 2 / 4 - 3 % 2;
    print result;

    isGreater: bool <- result > 5 && result < 20;
    print isGreater;

    print !isGreater || false;

    track: audio <- load "track.wav";
    sliced <- track[5:10] + track[20:25];
    play sliced;

    final: int <- 1;
    final += 2;
    final *= 3;
    print final;
}