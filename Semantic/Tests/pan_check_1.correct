panFunction <- (time: float): float {
    return SIN(time * 2 * 3.14);  // Oscillator
};

main {
    track: audio <- load "original.wav";
    pannedTrack <- PAN_DYNAMIC(track, panFunction);

    // Play and save panned track
    play pannedTrack;
    save pannedTrack -> "panned_track.wav";
}